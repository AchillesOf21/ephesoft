<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:rv='urn:import:com.ephesoft.dcma.gwt.rv.client.view'
	xmlns:sliding-panel='urn:import:com.ephesoft.dcma.gwt.core.client.view'>

	<g:DockLayoutPanel unit="PCT">
		<g:north size="13">
			<g:DockLayoutPanel unit="PCT">
				<g:north size="30">
					<g:HorizontalPanel>
						<g:Label ui:field="functionKeyLabel" />
					</g:HorizontalPanel>
				</g:north>
				<g:center size="30">
					<sliding-panel:SlidingPanel ui:field="slidingPanel">
						<g:DockLayoutPanel unit="PCT">
							<g:center size="5">
								<g:HorizontalPanel ui:field="firstShortcutsPanel" />
							</g:center>
						</g:DockLayoutPanel>
						<g:DockLayoutPanel unit="PCT">
							<g:center size="5">
								<g:HorizontalPanel ui:field="lastShortcutsPanel" />
							</g:center>
						</g:DockLayoutPanel>
					</sliding-panel:SlidingPanel>
				</g:center>
				<g:south size="2">
					<g:Label />
				</g:south>

			</g:DockLayoutPanel>
		</g:north>
		<g:center>
			<g:DockLayoutPanel unit="PCT">
				<g:north size="24">
					<g:FocusPanel width='100%' height='100%'>
						<g:VerticalPanel width='100%'>
							<rv:ReviewPanel ui:field="reviewPanel" />
						</g:VerticalPanel>
					</g:FocusPanel>
				</g:north>
				<g:south size="58">
					<g:LayoutPanel ui:field="layoutPanel">
						<g:layer>
							<rv:ValidatePanel ui:field="validatePanel" />
						</g:layer>
						<g:layer>
							<g:LayoutPanel ui:field="pageImageLayoutPanel">
							</g:LayoutPanel>

						</g:layer>
					</g:LayoutPanel>
				</g:south>
				<g:center>
					<g:VerticalPanel>
						<g:HorizontalPanel ui:field="urlButtonPanel" />
						<g:Label ui:field="seperator" />
					</g:VerticalPanel>
				</g:center>

			</g:DockLayoutPanel>

		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>
