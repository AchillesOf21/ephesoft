<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:core='com.ephesoft.dcma.gwt.core.client.validator'
	xmlns:v="urn:import:com.ephesoft.dcma.gwt.core.client.validator">

	<g:VerticalPanel width="100%">
	
		<g:FormPanel ui:field="exportFormPanel">
			<g:VerticalPanel width="100%" ui:field="exportBatchClassViewPanel">
			</g:VerticalPanel>
		</g:FormPanel>
	
		<g:VerticalPanel width="100%">
			<g:HorizontalPanel width="100%" ui:field="exportBatchPanel">
				<g:cell width="48%" horizontalAlignment="ALIGN_RIGHT">
					<g:Button ui:field="saveButton" />
				</g:cell>
				<g:cell width="5%">
					<g:Label></g:Label>
				</g:cell>
				<g:cell width="47%" horizontalAlignment="ALIGN_LEFT">
					<g:Button ui:field="cancelButton" />
				</g:cell>
			</g:HorizontalPanel>
		</g:VerticalPanel>
		
	</g:VerticalPanel>
</ui:UiBinder>
