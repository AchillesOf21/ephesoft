<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel width="100%" ui:field="editTableInfoViewPanel">

		<g:ScrollPanel ui:field="scrollPanel">
			<g:VerticalPanel width="100%" spacing="4">
				<g:HorizontalPanel width="100%">
					<g:cell width="20%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="nameLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="nameStar"></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="name" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="22%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="startPatternLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="startPatternStar"></g:Label>
					</g:cell>
					<g:cell width="16%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="startPattern" />
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT">
						<g:Button ui:field="startPatternValidateButton" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="22%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="endPatternLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="endPatternStar"></g:Label>
					</g:cell>
					<g:cell width="16%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="endPattern" />
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT">
						<g:Button ui:field="endPatternValidateButton" />
					</g:cell>
				</g:HorizontalPanel>


				<g:HorizontalPanel width="100%">
					<g:cell width="22%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="tableExtractionAPI"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="tableExtractionStar"></g:Label>
					</g:cell>
					<g:HorizontalPanel width="100%">
						<g:cell width="15%" horizontalAlignment="ALIGN_LEFT">
							<g:CheckBox ui:field="colCoordValidationCheckBox" />
						</g:cell>
						<g:cell width="5%" horizontalAlignment="ALIGN_LEFT">
							<g:ListBox ui:field="andOrListBox1" />
						</g:cell>
						<g:cell width="12%" horizontalAlignment="ALIGN_LEFT">
							<g:CheckBox ui:field="colHeaderValidationCheckBox" />
						</g:cell>
						<g:cell width="5%" horizontalAlignment="ALIGN_LEFT">
							<g:ListBox ui:field="andOrListBox2" />
						</g:cell>
						<g:cell width="30%" horizontalAlignment="ALIGN_LEFT">
							<g:CheckBox ui:field="regexValidationCheckBox" />
						</g:cell>
					</g:HorizontalPanel>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="22%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="widthOfMultiLineLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="77%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="widthOfMultiLine" />
					</g:cell>
				</g:HorizontalPanel>

			</g:VerticalPanel>
		</g:ScrollPanel>
		<g:HorizontalPanel width="100%" ui:field="editFieldPanel">

			<g:cell width="78%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="saveButton" />
			</g:cell>
			<g:cell width="1%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="7%">
				<g:Button ui:field="cancelButton" />
			</g:cell>
			<g:cell width="1%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="20%">
				<g:Button ui:field="samplePatternButton" />
			</g:cell>

		</g:HorizontalPanel>

	</g:VerticalPanel>
</ui:UiBinder>
