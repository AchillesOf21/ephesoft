<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:c='urn:import:com.ephesoft.dcma.gwt.core.client.ui'
	xmlns:core='urn:import:com.ephesoft.dcma.gwt.core.client.view'>

	<g:DockLayoutPanel unit='PCT' ui:field="advancedKVLayoutPanel">
		<g:west size='25'>
			<g:DockLayoutPanel unit='PCT'
				ui:field="groupingKVExtractionProperties">
				<g:north size="90">
					<g:DockLayoutPanel ui:field="properties" unit='PCT'>
						<g:north size="55">
							<g:VerticalPanel width="100%"
								ui:field="oldKVExtractionProperties">
								<g:HorizontalPanel width="100%">
									<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
										<g:CheckBox ui:field="useExistingKey"></g:CheckBox>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="keyFieldStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:Label ui:field="useExistingKeyLabel" />
									</g:cell>
								</g:HorizontalPanel>

								<g:HorizontalPanel width="100%">
									<g:cell width="48%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="keyPatternLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="keyPatternStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:HorizontalPanel ui:field="keyPatternPanel"
											width="98%" />
									</g:cell>
									<g:cell width="1%">
				                          <g:Label></g:Label>
			                        </g:cell>
			                        <g:cell >
										<g:Button ui:field="keyPatternValidateButton"/>
									</g:cell>
								</g:HorizontalPanel>

								<g:HorizontalPanel width="100%">
									<g:cell width="48%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="valuePatternLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="valuePatternStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:TextBox ui:field="valuePattern" />
									</g:cell>
									<g:cell width="1%">
				                          <g:Label></g:Label>
			                        </g:cell>
			                        <g:cell >
										<g:Button ui:field="valuePatternValidateButton"/>
									</g:cell>
								</g:HorizontalPanel>

								<g:HorizontalPanel width="100%">
									<g:cell width="44%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="multiplierLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="multiplierStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:TextBox ui:field="multiplier" />
									</g:cell>
								</g:HorizontalPanel>
								<g:HorizontalPanel width="100%">
									<g:cell width="44%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="fetchValueLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="fetchValueStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:ListBox ui:field="fetchValue" width="98%" />
									</g:cell>
								</g:HorizontalPanel>
								<g:HorizontalPanel width="100%">
									<g:cell width="44%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="kvPageValueLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="kvPageValueStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:ListBox ui:field="kvPageValue" width="98%" />
									</g:cell>
								</g:HorizontalPanel>
							</g:VerticalPanel>
						</g:north>

						<g:center>
							<g:VerticalPanel />
						</g:center>

						<g:south size="40">
							<g:VerticalPanel width="100%"
								ui:field="newKVExtractionProperties">

								<g:HorizontalPanel width="100%">
									<g:cell width="44%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="lengthLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="lengthStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:TextBox ui:field="length" />
									</g:cell>
								</g:HorizontalPanel>

								<g:HorizontalPanel width="100%">
									<g:cell width="44%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="widthLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="widthStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:TextBox ui:field="width" />
									</g:cell>
								</g:HorizontalPanel>

								<g:HorizontalPanel width="100%">
									<g:cell width="44%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="xOffsetLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="xOffsetStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:TextBox ui:field="xOffset" />
									</g:cell>
								</g:HorizontalPanel>

								<g:HorizontalPanel width="100%">
									<g:cell width="44%" horizontalAlignment="ALIGN_RIGHT">
										<g:Label ui:field="yOffsetLabel"></g:Label>
									</g:cell>
									<g:cell width="1%">
										<g:Label ui:field="yOffsetStar"></g:Label>
									</g:cell>
									<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
										<g:TextBox ui:field="yOffset" />
									</g:cell>
								</g:HorizontalPanel>
							</g:VerticalPanel>
						</g:south>
					</g:DockLayoutPanel>
				</g:north>

				<g:center>
					<g:VerticalPanel />
				</g:center>

				<g:south size="10">
					<g:HorizontalPanel ui:field="editAdvancedKVPanel">
						<g:cell width="1%">
							<g:Label></g:Label>
						</g:cell>
						<g:Button ui:field="saveButton" />
						<g:cell width="2%">
							<g:Label></g:Label>
						</g:cell>
						<g:Button ui:field="cancelButton" />
						<g:cell width="2%">
							<g:Label></g:Label>
						</g:cell>
						<g:Button ui:field="samplePatternButton" />
						
					</g:HorizontalPanel>
				</g:south>
			</g:DockLayoutPanel>
		</g:west>
		<g:center>
			<g:DockLayoutPanel unit="PCT" ui:field="imageOverlayPanel">
				<g:north size="7">
					<g:DockLayoutPanel unit="PCT" styleName="button_image_overlay">
						<g:west size="33">
							<g:FormPanel ui:field="imageUpload">
								<g:HorizontalPanel width="100%">
									<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
										<g:FileUpload ui:field="importFile" name="importFile">
										</g:FileUpload>
									</g:cell>
									<g:cell>
										<g:Hidden ui:field="batchClassID" name="batchClassID" />
									</g:cell>
									<g:cell>
										<g:Hidden ui:field="docName" name="docName" />
									</g:cell>
								</g:HorizontalPanel>
							</g:FormPanel>
						</g:west>
						<g:center>
							<g:DockLayoutPanel>
								<g:west size="0">
									<c:RotatableImage ui:field="tempImage"
										visible="false" />
								</g:west>
								<g:center>
									<core:SlidingPanel ui:field="optionsSlidingPanel">
										<g:HorizontalPanel ui:field="initialOptionsPanel">
											<g:Button ui:field="captureKey" />
											<g:Button ui:field="editKey" />
											<g:Button ui:field="captureValue" />
											<g:Button ui:field="editValue" />
											<g:Button ui:field="nextOptions" />
										</g:HorizontalPanel>
										<g:HorizontalPanel ui:field="secondOptionsPanel">
											<g:Button ui:field="previousOptions" />
											<g:Button ui:field="clearButton" />
											<g:Button ui:field="testAdvKvButton" />
										</g:HorizontalPanel>
									</core:SlidingPanel>
								</g:center>
							</g:DockLayoutPanel>
						</g:center>
					</g:DockLayoutPanel>
				</g:north>

				<g:center>
					<g:ScrollPanel ui:field="imageScroll">
						<g:HorizontalPanel width="100%">
							<g:cell width="100%" height="100%" horizontalAlignment='ALIGN_CENTER'>
								<c:RotatableImage ui:field="pageImage" />
							</g:cell>
						</g:HorizontalPanel>
					</g:ScrollPanel>
				</g:center>
			</g:DockLayoutPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>

