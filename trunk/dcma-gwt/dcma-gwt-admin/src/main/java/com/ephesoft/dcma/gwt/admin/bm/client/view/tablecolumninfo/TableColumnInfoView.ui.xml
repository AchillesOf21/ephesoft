<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:table-column-info='urn:import:com.ephesoft.dcma.gwt.admin.bm.client.view.tablecolumninfo'>

	<g:DockLayoutPanel unit='PCT'>
		<g:north size="43">
			<g:CaptionPanel ui:field="tcInfoConfigurationCaptionPanel"
				height="80%" width="97%">
				<g:ScrollPanel height="100%" width="100%">
					<g:VerticalPanel height="100%" width="100%">
						<g:VerticalPanel ui:field="tcInfoVerticalPanel"
							height="100%" width="100%">
							<table-column-info:TableColumnInfoDetailView
								ui:field="tableColumnInfoDetailView" width='100%' height="100%" />
							<g:HorizontalPanel width="100%">
								<g:cell width="100%" horizontalAlignment="ALIGN_RIGHT">
									<g:Button ui:field="editTCInfoPropertiesButton"></g:Button>
								</g:cell>
							</g:HorizontalPanel>
						</g:VerticalPanel>

						<g:VerticalPanel ui:field="tcInfoConfigVerticalPanel"
							height="100%" visible="false" width="100%">
							<table-column-info:EditTableColumnInfoView
								ui:field="editTableColumnInfoView" width='100%' />
						</g:VerticalPanel>
					</g:VerticalPanel>
				</g:ScrollPanel>
			</g:CaptionPanel>
		</g:north>
	</g:DockLayoutPanel>

</ui:UiBinder>
