<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel width="100%">
		<g:ScrollPanel ui:field="scrollPanel">
			<g:VerticalPanel width="100%" spacing="4">
				<g:FlexTable ui:field="viewTable" width="100%" height="70%">
				</g:FlexTable>
			</g:VerticalPanel>
		</g:ScrollPanel>
		<g:HorizontalPanel width="100%">
			<g:cell width="75%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="cmisInfo" visible="false"></g:Button>
			</g:cell>
			<g:cell width="1%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="95%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="testCMIS"></g:Button>
			</g:cell>
			<g:cell width="1%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="4%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="edit"></g:Button>
			</g:cell>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>
