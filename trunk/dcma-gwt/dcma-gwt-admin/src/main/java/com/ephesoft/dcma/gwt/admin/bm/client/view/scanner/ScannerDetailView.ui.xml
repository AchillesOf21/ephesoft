<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel width="100%">
		<g:ScrollPanel ui:field="scrollPanel">
			<g:VerticalPanel width="100%" spacing="4">

				<g:HorizontalPanel width="100%">
					<g:FlexTable ui:field="flexEditTable" width="100%"
						height="70%">
					</g:FlexTable>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:ScrollPanel>
		<g:HorizontalPanel width="100%">
			<g:cell width="100%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="editScannerPropertiesButton"></g:Button>
			</g:cell>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>   
