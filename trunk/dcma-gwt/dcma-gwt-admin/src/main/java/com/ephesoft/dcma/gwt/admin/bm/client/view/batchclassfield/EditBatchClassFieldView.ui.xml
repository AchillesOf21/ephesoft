<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel width="100%" ui:field="editBatchClassFieldViewPanel">
		
		<g:ScrollPanel ui:field="scrollPanel">
			<g:VerticalPanel width="100%" spacing="4">
			
				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="nameLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="nameStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="name" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="descriptionLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="descriptionStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="description" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="dataTypeLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="dataTypeStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:ListBox ui:field="dataType" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="fieldOrderNumberLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="fieldOrderNumberStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="fieldOrderNumber" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="sampleValueLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="sampleValue" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="validationPatternLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="16%" horizontalAlignment="ALIGN_LEFT">
				<g:TextBox ui:field="validationPattern" />
			</g:cell>
			<g:cell  horizontalAlignment="ALIGN_LEFT">
				<g:Button ui:field="validateButton" />
			</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="fieldOptionValueListLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="fieldOptionValueList" />
					</g:cell>
				</g:HorizontalPanel>

			</g:VerticalPanel>
		</g:ScrollPanel>
		<g:HorizontalPanel width="100%" ui:field="editBatchClassFieldPanel">
			<g:cell width="78%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="saveButton" />
			</g:cell>
			<g:cell width="2%">
				<g:Label></g:Label>
			</g:cell>
			
			<g:cell width="2%">
				<g:Button ui:field="cancelButton" />
			</g:cell>
			<g:cell width="2%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="13%">
			<g:Button ui:field="samplePatternButton" />
			</g:cell>
			
		</g:HorizontalPanel>

	</g:VerticalPanel>
</ui:UiBinder>
