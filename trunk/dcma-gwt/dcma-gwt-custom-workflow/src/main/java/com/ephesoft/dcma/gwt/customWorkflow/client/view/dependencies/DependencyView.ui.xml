<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:cw='urn:import:com.ephesoft.dcma.gwt.customworkflow.client.view'>
	<g:DockLayoutPanel ui:field="dependencyLayoutPanel"
		width="98%" height="98%">
		<g:north size="40">
			<g:VerticalPanel width = "100%">
				<g:HorizontalPanel width="100%" height="100%" ui:field="dependenciesButtonPanel">
					<g:cell>
						<g:Label ui:field="pluginLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="20%" horizontalAlignment="ALIGN_RIGHT">
						<g:ListBox ui:field="pluginNames"></g:ListBox>
					</g:cell>
					<g:cell width="75%" horizontalAlignment="ALIGN_RIGHT">
						<g:Button ui:field="addDependencyButton"></g:Button>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_RIGHT">
						<g:Button ui:field="editDependencyButton"></g:Button>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_RIGHT">
						<g:Button ui:field="deleteDependencyButton"></g:Button>
					</g:cell>
				</g:HorizontalPanel>
			</g:VerticalPanel>

		</g:north>
		<g:center>
			<g:LayoutPanel ui:field="dependencyListPanel">
			</g:LayoutPanel>
		</g:center>

	</g:DockLayoutPanel>
</ui:UiBinder>