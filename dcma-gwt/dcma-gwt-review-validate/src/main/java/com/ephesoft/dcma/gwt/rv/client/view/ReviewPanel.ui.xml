	<g:VerticalPanel width='100%'>
		<g:FocusPanel width='100%' height='100%'>
			<g:VerticalPanel>
				<g:Label ui:field="docTypeText"></g:Label>
				<g:Label height="3px"></g:Label>
				<g:VerticalPanel ui:field="documentTypePanel" width="100%" />
				<g:Label height="8px"></g:Label>
				<g:Label ui:field="mergeDocText"></g:Label>
				<g:Label height="3px"></g:Label>
				<g:ListBox ui:field="documentList" width="100%" />
			</g:VerticalPanel>
		</g:FocusPanel>
	</g:VerticalPanel>

</ui:UiBinder>
