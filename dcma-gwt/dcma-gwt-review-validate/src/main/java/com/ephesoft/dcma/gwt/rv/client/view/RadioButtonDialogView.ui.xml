<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:core='com.ephesoft.dcma.gwt.core.client.validator'
	xmlns:v="urn:import:com.ephesoft.dcma.gwt.core.client.validator">

	<g:HTMLPanel>
		<div>
			<g:VerticalPanel width="100%" ui:field="radioButtonDialogViewPanel">
				<g:VerticalPanel ui:field="radioPanel">
				</g:VerticalPanel>
				<g:HorizontalPanel ui:field="buttonPanel">
					<g:cell horizontalAlignment="ALIGN_LEFT">
						<g:Button ui:field="okButton">
						</g:Button>
					</g:cell>
					<g:cell width="7%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT">
						<g:Button ui:field="cancelButton">
						</g:Button>
					</g:cell>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
