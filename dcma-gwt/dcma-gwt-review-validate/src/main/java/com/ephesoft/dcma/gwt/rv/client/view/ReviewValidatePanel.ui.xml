<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:rv='urn:import:com.ephesoft.dcma.gwt.rv.client.view'
	xmlns:sliding-panel='urn:import:com.ephesoft.dcma.gwt.core.client.view'>

		<g:DockLayoutPanel unit="PCT">
		<g:north size="10">
			<g:DockLayoutPanel unit="PCT">
				<g:north size="45">
					<g:DockLayoutPanel unit="PCT">
						<g:north size="20">
							<g:HorizontalPanel>
								<g:cell width="10%">
									<g:Label />
								</g:cell>
							</g:HorizontalPanel>
						</g:north>
						<g:west size="2">
							<g:HorizontalPanel>
								<g:cell>
									<g:Label />
								</g:cell>
							</g:HorizontalPanel>
						</g:west>
						<g:center>
							<g:HorizontalPanel>
								<g:cell width="90%">
									<g:Label ui:field="functionKeyLabel" />
								</g:cell>
							</g:HorizontalPanel>
						</g:center>
					</g:DockLayoutPanel>
				</g:north>
				<g:west size="2">
					<g:HorizontalPanel>
						<g:cell>
							<g:Label />
						</g:cell>
					</g:HorizontalPanel>
				</g:west>
				<g:center size="30">
					<sliding-panel:SlidingPanel ui:field="slidingPanel">
						<g:DockLayoutPanel unit="PCT">
							<g:center size="5">
								<g:HorizontalPanel ui:field="firstShortcutsPanel" />
							</g:center>
						</g:DockLayoutPanel>
						<g:DockLayoutPanel unit="PCT">
							<g:center size="5">
								<g:HorizontalPanel ui:field="lastShortcutsPanel" />
							</g:center>
						</g:DockLayoutPanel>
					</sliding-panel:SlidingPanel>
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:center>
			<g:DockLayoutPanel unit="PCT" ui:field="reviewValidateDockLayoutPanel" />
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>
