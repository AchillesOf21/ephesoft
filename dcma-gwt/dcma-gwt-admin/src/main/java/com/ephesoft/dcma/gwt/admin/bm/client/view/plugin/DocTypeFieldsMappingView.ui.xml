<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:module='urn:import:com.ephesoft.dcma.gwt.admin.bm.client.view.module'
	xmlns:plugin='urn:import:com.ephesoft.dcma.gwt.admin.bm.client.view.plugin'>

	<g:DockLayoutPanel unit='PCT'>
		<g:north size="30">
			<g:CaptionPanel ui:field="documentTypeDetails" height="80%"
				width="97%">
				<g:ScrollPanel width="100%" height="100%">
					<g:VerticalPanel width="100%" height="100%">
						<plugin:FuzzyDBDocTypeDetailView
							ui:field="fuzzyDBDocTypeDetailView" width='100%' height="100%" />
					</g:VerticalPanel>
				</g:ScrollPanel>
			</g:CaptionPanel>
		</g:north>
		<g:center>
			<g:CaptionPanel ui:field="fieldMapping" height="93%"
				width="97%">
				<g:ScrollPanel height="100%" width="100%">
					<g:FlexTable ui:field="flexEditTable" width="100%"
						height="100%" />
				</g:ScrollPanel>
			</g:CaptionPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
