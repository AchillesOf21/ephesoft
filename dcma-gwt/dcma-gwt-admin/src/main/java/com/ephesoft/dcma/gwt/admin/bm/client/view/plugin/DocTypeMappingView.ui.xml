<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:module='urn:import:com.ephesoft.dcma.gwt.admin.bm.client.view.module'
	xmlns:plugin='urn:import:com.ephesoft.dcma.gwt.admin.bm.client.view.plugin'>

	<g:DockLayoutPanel unit='PCT'>
		<g:north size="30">
			<g:CaptionPanel ui:field="pluginDetailsPanel"
				height="80%" width="97%">
						<g:ScrollPanel height="100%" width="100%">
									<g:VerticalPanel width="100%" height="100%">
									<plugin:PluginData ui:field="pluginData" width='100%' height="100%" />
									</g:VerticalPanel>
						</g:ScrollPanel>
					
			</g:CaptionPanel>
		</g:north>
		<g:center>
			<g:CaptionPanel ui:field="dbMappingPanel" width='97%'
				height="93%">
				<g:ScrollPanel height="100%" width="100%">
					<g:FlexTable ui:field="flexEditTable" width="100%" height="100%" />
				</g:ScrollPanel>
			</g:CaptionPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
