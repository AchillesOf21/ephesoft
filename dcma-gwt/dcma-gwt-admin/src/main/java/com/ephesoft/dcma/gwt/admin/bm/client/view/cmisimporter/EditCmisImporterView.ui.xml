<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel width="100%" ui:field="editCmisViewPanel">
		<g:ScrollPanel ui:field="scrollPanel">
			<g:VerticalPanel width="100%" spacing="4">
				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="serverUrlLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="serverUrlStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="serverUrl" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="userNameLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="userNameStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="userName" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="passwordLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="passwordStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="password" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="repositoryIDLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="repositoryIDStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="repositoryID" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="fileExtensionLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="fileExtensionStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="fileExtension" readOnly="true"
							value="pdf;tif" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="folderNameLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="folderNameStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="folderName" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="cmisPropertyLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="cmisPropertyStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="cmisProperty" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="valueLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="valueStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="value" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="valueToUpdateLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label ui:field="valueToUpdateStar"></g:Label>
					</g:cell>
					<g:cell width="60%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="valueToUpdate" />
					</g:cell>
				</g:HorizontalPanel>

			</g:VerticalPanel>
		</g:ScrollPanel>
		<g:HorizontalPanel width="100%" ui:field="editCmisPanel">
			<g:cell width="90%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="saveButton" />
			</g:cell>
			<g:cell width="1%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="7%">
				<g:Button ui:field="cancelButton" />
			</g:cell>
		</g:HorizontalPanel>

	</g:VerticalPanel>
</ui:UiBinder>
