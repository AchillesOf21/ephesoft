<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>


	<g:VerticalPanel width="100%">
		<g:ScrollPanel height="16em" width="100%">
			<g:VerticalPanel width="100%" spacing="4">
				<g:FlexTable ui:field="editTable" width="100%" height="70%">
				</g:FlexTable>
			</g:VerticalPanel>
		</g:ScrollPanel>
		<g:HorizontalPanel width="100%" ui:field="buttonPanel">
			<g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="authenticationTokenButton"></g:Button>
			</g:cell>

			<!--  g:cell width="40%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="authenticateButton"></g:Button>
			</g:cell>
			-->

			<g:cell width="1%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="4%">
				<g:Button ui:field="ok"></g:Button>
			</g:cell>
			<g:cell width="1%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="7%">
				<g:Button ui:field="cancel"></g:Button>
			</g:cell>

		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>
