<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel ui:field="editPanel" width="100%">
		<g:ScrollPanel height="6em" width="100%">
			<g:VerticalPanel width="100%" spacing="4">
				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="nameLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="name" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="descLabel">Description:</g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="description" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="remoteUrlLabel">Remote URL:</g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="remoteUrl" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="remoteBatchClassIdentifierLabel">Remote URL:</g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:TextBox ui:field="remoteBatchClassIdentifier" />
					</g:cell>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:ScrollPanel>
		<g:HorizontalPanel width="100%" ui:field="editModulePanel">
			<g:cell width="90%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="saveButton" />
			</g:cell>
			<g:cell width="1%">
				<g:Label></g:Label>
			</g:cell>
			<g:cell width="7%">
				<g:Button ui:field="cancelButton" />
			</g:cell>

		</g:HorizontalPanel>

	</g:VerticalPanel>

</ui:UiBinder>
