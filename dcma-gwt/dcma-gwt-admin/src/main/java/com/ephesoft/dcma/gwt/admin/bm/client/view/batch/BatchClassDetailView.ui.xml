<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel height="100%" width="100%">
		<g:ScrollPanel ui:field="scrollPanel" height="8em"
			width="100%">
			<g:VerticalPanel width="100%" spacing="3">
				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="nameLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:Label ui:field="name" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="descLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:Label ui:field="description" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="priorityLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:Label ui:field="priority" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="uncLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:Label ui:field="uncFolder" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="systemFolderLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:Label ui:field="systemFolder" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="versionLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:Label ui:field="version" />
					</g:cell>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%">
					<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
						<g:Label ui:field="roleLabel"></g:Label>
					</g:cell>
					<g:cell width="1%">
						<g:Label></g:Label>
					</g:cell>
					<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
						<g:Label ui:field="role" />
					</g:cell>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:ScrollPanel>
		<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_RIGHT">
			<g:cell width="98%">
				<g:Label></g:Label>
			</g:cell>
			<g:Button ui:field="editBatchPropertiesButton"></g:Button>
			<g:cell width="3%">
				<g:Label></g:Label>
			</g:cell>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>
