<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:cw='urn:import:com.ephesoft.dcma.gwt.core.client.view'>

	<g:DockLayoutPanel ui:field="customWorkflowPanel"
		unit="PCT">
		<g:south size="30">
			<g:VerticalPanel ui:field="buttonsVerticalPanel">
				<g:HorizontalPanel spacing="20">
					<g:Button ui:field="addNewModule"></g:Button>
				</g:HorizontalPanel>
				<g:HorizontalPanel spacing="20">
					<g:cell width="75%" horizontalAlignment="ALIGN_LEFT">
						<g:Button ui:field="okButton"></g:Button>
					</g:cell>
					<g:cell width="10%">
						<g:Label>
						</g:Label>
					</g:cell>
					<g:cell width="25%" horizontalAlignment="ALIGN_RIGHT">
						<g:Button ui:field="resetButton"></g:Button>
					</g:cell>
					<g:cell width="10%">
						<g:Label>
						</g:Label>
					</g:cell>
					<g:cell width="25%" horizontalAlignment="ALIGN_RIGHT">
						<g:Button ui:field="cancelButton"></g:Button>
					</g:cell>

				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder>
