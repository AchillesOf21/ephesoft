<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel width="100%" ui:field="editRegexViewPanel">

		<g:HorizontalPanel width="100%">
			<g:cell width="10%" horizontalAlignment="ALIGN_RIGHT">
				<g:Label ui:field="patternLabel"></g:Label>
			</g:cell>
			<g:cell width="1%">
				<g:Label ui:field="patternStar"></g:Label>
			</g:cell>
			<g:cell width="70%" horizontalAlignment="ALIGN_LEFT">
				<g:TextBox ui:field="pattern" />
			</g:cell>
		</g:HorizontalPanel>

		<g:HorizontalPanel ui:field="editKVPanel">
			<g:Button ui:field="saveButton" />
			<g:cell width="7%">
				<g:Label></g:Label>
			</g:cell>
			<g:Button ui:field="cancelButton" />
			<g:cell width="7%">
				<g:Label></g:Label>
			</g:cell>
			<g:Button ui:field="samplePatternButton" />
		</g:HorizontalPanel>

	</g:VerticalPanel>
</ui:UiBinder>
